{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,   
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "@components/*": [
          "./src/Components/*"
        ],
        "@utils/*": [
          "./src/Utils/*"
        ],
        "@assets/*": [
          "./src/Assets/*"
        ],
        "@pages/*": [
          "./src/Pages/*"
        ],
        "@services/*": [
          "./src/Services/*"
        ],
        "@useHookStore/*": [
          "./src/Store/hooks/*"
        ],
      }
    
  },
  "extends": "expo/tsconfig.base"
}
