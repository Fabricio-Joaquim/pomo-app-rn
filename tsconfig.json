{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,   
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "@components/*": [
          "./src/Components/*"
        ],
        "@utils/*": [
          "./src/Utils/*"
        ],
        "@assets/*": [
          "./src/Assets/*"
        ],
        "@screens/*": [
          "src/Screens/*"
        ],
        "@services/*": [
          "./src/Services/*"
        ],
        "@useHookStore/*": [
          "./src/Store/hooks/*"
        ],
        "@enums/*": [
          "./src/Enums/*"
        ],
        "@interfaces/*": [
          "./src/Interfaces/*"
        ],
        "@useHooks/*": [
          "./src/hooks/*"
        ],
      },
      "typeRoots": [
        "node_modules/@types",
        "src",
        "./src/types/*",
        "./src/types/navigate.d.ts",
      ],
  },
  "extends": "expo/tsconfig.base"
}
